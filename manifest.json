{
  "manifest_version": 3,
  "name": "Page Load Listener Extension",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://*.douyin.com/*",
    "http://*/",
    "https://*/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.douyin.com/*",
        "http://*/",
        "https://*/"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}